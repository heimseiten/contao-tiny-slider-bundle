{
    "name": "heimseiten/contao-tiny-slider-bundle",
    "description": "Ersetzt den standard Contao-Slider mit dem Tiny Slider und fügt Funktionen wie Fade, Ken-Burns-Effekt uvm. hinzu (https://github.com/ganlanyuan/tiny-slider).",
    "keywords": [
        "slider"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "heimseiten.de - Webdesign aus Köln",
            "email": "info@heimseiten.de",
            "homepage": "https://www.heimseiten.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimseiten/contao-tiny-slider-bundle/issues",
        "source": "https://github.com/heimseiten/contao-tiny-slider-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.4 || ^5.0"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^3.0",
        "contao/manager-plugin": "^2.3",
        "contao/test-case": "^4.0",
        "phpunit/phpunit": "^8.4",
        "symfony/http-client": "4.4.*",
        "symfony/phpunit-bridge": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Heimseiten\\ContaoTinySliderBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Heimseiten\\ContaoTinySliderBundle\\ContaoManager\\Plugin"
    }
}
